    "files.associations": {
        "*.env": "shellscript",
        "**/templates/**/*.html": "django-html",
        "**/requirements{/**,*}.{txt,in}": "pip-requirements"
    },
    "emmet.includeLanguages": {
        "django-html": "html"
    },
    "git.autoRepositoryDetection": "openEditors",
    "debug.toolBarLocation": "docked"
}